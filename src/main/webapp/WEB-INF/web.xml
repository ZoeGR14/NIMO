<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ControlerJ</servlet-name>
        <servlet-class>Controlador.ControlerJ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlerZ</servlet-name>
        <servlet-class>Controlador.ControlerZ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlerZz</servlet-name>
        <servlet-class>Controlador.ControlerZz</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlerZzZ</servlet-name>
        <servlet-class>Controlador.ControlerZzZ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControlerJ</servlet-name>
        <url-pattern>/ControlerJ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerZ</servlet-name>
        <url-pattern>/ControlerZ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerZz</servlet-name>
        <url-pattern>/ControlerZz</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerZzZ</servlet-name>
        <url-pattern>/ControlerZzZ</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ControladorImagen</servlet-name>
        <servlet-class>Controlador.ControlerImagen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controler</servlet-name>
        <servlet-class>Controlador.Controler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlerIMG</servlet-name>
        <servlet-class>Controlador.ControlerIMG</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorImagen</servlet-name>
        <url-pattern>/ControladorImagen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controler</servlet-name>
        <url-pattern>/Controler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerIMG</servlet-name>
        <url-pattern>/ControlerIMG</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <resource-ref>
        <res-ref-name>888</res-ref-name>
        <res-type>javax.mail.Session</res-type>
        <res-auth>Container</res-auth>
        <res-sharing-scope>Shareable</res-sharing-scope>
    </resource-ref>
</web-app>
